{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++-9 build project",
      "command": "/usr/bin/g++-9",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${workspaceFolder}/grafo.cc",
        "${workspaceFolder}/main.cc",
        "-o",
        "${workspaceFolder}/grafo.out"
