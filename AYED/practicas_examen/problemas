bool is_simetric(C[m][m]) {
  simetrico = true;
  for (int i = 1; i <= m && simetrico; ++i) {
    for (int j = 1; j <= m && simetrico; ++j) {
      if (C[i][j] != C[j][i]) simetrico = false;
    }
  }
  return simetrico;
}


bool antisimetric(C[m][m]) {
  simetrico = true;
  for (int i = 1; i <= m && simetrico; ++i) {
    for (int j = 1; j <= m && simetrico; ++j) {
      if ((C[i][j]) != -(C[j][i])) simetrico = false;
    }
  }
  return simetrico;
}